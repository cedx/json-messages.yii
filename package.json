  "author": "CÃ©dric Belin <cedric@belin.io>",
  "bugs": "https://github.com/cedx/json-messages.yii/issues",
  "description": "JSON message source for Yii Framework.",
  "homepage": "https://packagist.org/packages/cedx/yii2-json-messages",
  "license": "MIT",
  "name": "yii2-json-messages",
  "private": true,
  "version": "0.3.0",
  "devDependencies": {
    "del": "^1.1.1",
    "docgen-js": "^0.0.1",
    "gulp": "^3.8.11",
    "gulp-david": "^0.2.0",
    "gulp-jshint": "^1.9.2",
    "gulp-load-plugins": "^0.8.0",
    "gulp-zip": "^2.0.2"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "jshintConfig": {
    "camelcase": true,
    "freeze": true,
    "indent": 2,
    "latedef": "nofunc",
    "loopfunc": true,
    "newcap": true,
    "node": true,
    "nonew": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "yuidoc": {
    "logo": "http://dev.belin.io/json-messages.yii/favicon.ico",
    "name": "JSON-Messages.yii",
    "url": "https://packagist.org/packages/cedx/yii2-json-messages",
    "options": {
      "extension": ".php",
      "outdir": "doc/api",
      "paths": "lib",
      "tabtospace": 2,
      "theme": "spacelab"
    }
  }
}
